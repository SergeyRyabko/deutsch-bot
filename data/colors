schwer :: тяжелый
lang :: длинный
schön :: красивый
dick :: толстый
modisch :: модный
praktisch :: практичный
bequem :: удобный
leicht :: легкий
kurz :: короткий 
hässlich :: некрасивый 
dünn :: тонкий
blau :: синий, голубой
braun :: коричневый 
gelb :: желтый 
grau :: серый
grün :: зеленый
rot :: красный 
schwarz :: черный 
weiß :: белый
